-- User
--     ident Text
--     password Text Maybe
--     UniqueUser ident
--     deriving Typeable
-- Email
--     email Text
--     user UserId Maybe
--     verkey Text Maybe
--     UniqueEmail email

User
    starExecId Text
    spaces [SpaceId]
    UniqueUser starExecId
    deriving Typeable Show

Space
    starExecId Text
    UniqueSpace starExecId
    users [UserId]
    deriving Typeable Show

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
