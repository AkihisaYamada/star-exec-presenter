User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable
-- Email
--     email Text
--     user UserId Maybe
--     verkey Text Maybe
--     UniqueEmail email

StarExecSessionData
  key Int
  UniqueStarExecSessionData key
  cookies Text
  date UTCTime

JobResultInfo
    jobId Int
    score Int Maybe
    pairId Int
    UniqueJobResultInfo pairId
    benchmark Text
    benchmarkId Int
    solver Text
    solverId Int
    configuration Text
    configurationId Int
    status JobResultStatus
    cpuTime Double
    wallclockTime Double
    result SolverResult
    deriving Show

JobPairInfo
    pairId Int
    UniqueJobPairInfo pairId
    stdout ByteString
    log ByteString
    htmlProof ByteString Maybe
    resultStatus JobResultStatus default='JobResultUndetermined'
    deriving Show

JobInfo
    starExecId Int
    UniqueJobInfo starExecId
    name Text
    status JobStatus
    date Text
    preProc Text default='unknown'
    postProc Text default='unknown'
    isComplexity Bool default=FALSE
    lastUpdate UTCTime default=now()
    deriving Show

SolverInfo
    starExecId Int
    UniqueSolverInfo starExecId
    name Text
    description Text
    lastUpdate UTCTime default=now()
    deriving Show

BenchmarkInfo
    starExecId Int
    UniqueBenchmarkInfo starExecId
    name Text
    type Text
    lastUpdate UTCTime default=now()
    deriving Show

QueryInfo
    query SEQuery
    UniqueQueryInfo query
    deriving Show

CompetitionInfo
    competition Competition
    date UTCTime default=now()
    public Bool
    deriving Show

