User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable
-- Email
--     email Text
--     user UserId Maybe
--     verkey Text Maybe
--     UniqueEmail email

StarExecSessionData
  key Int
  UniqueStarExecSessionData key
  cookies Text
  date UTCTime

JobResultInfo
    jobId Int
    score Int Maybe
    pairId Int
    UniqueJobResultInfo pairId
    benchmark Text
    benchmarkId Int
    solver Text
    solverId Int
    configuration Text
    configurationId Int
    status JobResultStatus
    cpuTime Double
    wallclockTime Double
    result SolverResult
    deriving Show

LriResultInfo
    jobId Text
    score Int Maybe
    pairId Text
    UniqueLriResultInfo pairId
    benchmarkId Text
    solverId Text
    result SolverResult
    cpuTime Double
    wallclockTime Double
    checkResult SolverResult Maybe
    checkCpuTime Double Maybe
    checkWallclockTime Double Maybe
    deriving Show

JobPairInfo
    pairId Int
    UniqueJobPairInfo pairId
    stdout ByteString
    log ByteString
    htmlProof ByteString Maybe
    resultStatus JobResultStatus default='JobResultUndetermined'
    deriving Show

JobInfo
    starExecId Int
    UniqueJobInfo starExecId
    name Text
    status JobStatus
    date Text
    preProc Text default='unknown'
    postProc Text default='unknown'
    isComplexity Bool default=FALSE
    isPublic Bool default=TRUE
    startDate UTCTime default=now()
    finishDate UTCTime Maybe
    lastUpdate UTCTime default=now()
    deriving Show

SolverInfo
    starExecId Int
    UniqueSolverInfo starExecId
    name Text
    description Text
    lastUpdate UTCTime default=now()
    deriving Eq Ord Read Show

LriSolverInfo
    solverId Text
    UniqueLriSolverInfo solverId
    name Text
    author Text
    description Text
    url Text
    isStandard Bool
    isRelative Bool
    isConditional Bool
    isContextSensitive Bool
    isInnermost Bool
    isTheory Bool
    isCertifying Bool
    deriving Eq Ord Read Show

BenchmarkInfo
    starExecId Int
    UniqueBenchmarkInfo starExecId
    name Text
    type Text
    lastUpdate UTCTime default=now()
    deriving Eq Ord Read Show

LriBenchmarkInfo
    benchmarkId Text
    UniqueLriBenchmarkInfo benchmarkId
    name Text
    file Text
    rating Int
    solved Int
    isConditional Bool
    isContextSensitive Bool
    isInnermost Bool
    isOutermost Bool
    isRelative Bool
    isTheory Bool
    deriving Eq Ord Read Show

QueryInfo
    query SEQuery
    UniqueQueryInfo query
    deriving Show

CompetitionInfo
    competition Competition
    date UTCTime default=now()
    public Bool default=True
    deriving Show

PostProcInfo
    starExecId Int
    UniquePostProcInfo starExecId
    name Text
    description Text
    lastUpdate UTCTime default=now()
    deriving Show

