--     UniqueEmail email

PersistJobResultInfo
    starExecJobId Int
    pairId Int
    UniquePersistJobResultInfo pairId
    benchmark Text
    benchmarkId Int
    solver Text
    solverId Int
    configuration Text
    configurationId Int
    status Text
    cpuTime Double
    wallclockTime Double
    result SolverResult
    deriving Show

PersistJobPairInfo
    pairId Int
    UniquePersistJobPairInfo pairId
    stdout ByteString
    log ByteString

PersistJobInfo
    starExecId Int
    UniquePersistJobInfo starExecId
    spaceId Int
    name Text
    status JobStatus
    pairsCompleted Double
    pairsNum Int
    pairsFailed Double
    date Text
    deriving Show

PersistSolverInfo
    starExecId Int
    UniquePersistSolverInfo starExecId
    name Text
    description Text
    deriving Show

PersistBenchmarkInfo
    starExecId Int
    UniquePersistBenchmarkInfo starExecId
    name Text
    type Text
    deriving Show

PersistUserInfo
    starExecId Int
    UniquePersistUserInfo starExecId
    name Text
    institution Text
    mail Text

PersistSpaceInfo
    starExecId Int
    UniquePersistSpaceInfo starExecId
    parentId Int Maybe
    name Text
    description Text

