--     email Text
--     user UserId Maybe
--     verkey Text Maybe
--     UniqueEmail email

StarExecSessionData
  key Int
  UniqueStarExecSessionData key
  cookies Text
  date UTCTime

JobResultInfo
    jobId Int
    pairId Int
    UniqueJobResultInfo pairId
    benchmark Text
    benchmarkId Int
    solver Text
    solverId Int
    configuration Text
    configurationId Int
    status Text
    cpuTime Double
    wallclockTime Double
    result SolverResult
    deriving Show

JobPairInfo
    pairId Int
    UniqueJobPairInfo pairId
    stdout ByteString
    log ByteString
    deriving Show

JobInfo
    starExecId Int
    UniqueJobInfo starExecId
    name Text
    status JobStatus
    date Text
    lastUpdate UTCTime
    deriving Show

SolverInfo
    starExecId Int
    UniqueSolverInfo starExecId
    name Text
    description Text
    lastUpdate UTCTime
    deriving Show

BenchmarkInfo
    starExecId Int
    UniqueBenchmarkInfo starExecId
    name Text
    type Text
    lastUpdate UTCTime
    deriving Show

QueryInfo
    query SEQuery
    UniqueQueryInfo query
    deriving Show

CompetitionInfo
    competition Competition
    date UTCTime
    deriving Show

