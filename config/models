JobResult
    starExecJobId Int
    pairId Int
    UniqueJobResult pairId
    benchmark Text
    benchmarkId Int
    solver Text
    solverId Int
    configuration Text
    configurationId Int
    status Text
    cpuTime Double
    wallclockTime Double
    result SolverResult
    deriving Show

Job
    starExecId Int
    UniqueJob starExecId
    status JobStatus
    deriving Show

